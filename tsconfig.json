{
  "include": ["remix.env.d.ts", "**/*.ts", "**/*.tsx", "prisma/seed.js"],
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ES2019"],
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "target": "ES2019",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "module": "ES2020",
    "paths": { "~/*": ["./app/*"] }
  }
}
